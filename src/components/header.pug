mixin header()
  header.yul-header 
    .wrapper.yul-header__wrapper
      a(href="./home.html").yul-header__logo
        +logo()
      .yul-header__block
        .yul-header__navigation 
          +navigation() 
        //- .yul-header__burger 
        //-   +burger()
        .yul-header__button
          +button({
            type: 'button',
            text: 'Subscribe',
            toolTip: 'Push for Subscribe',
            whiteMode: true
          })