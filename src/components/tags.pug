mixin tagsTable ()
  section.val-tags-table 
    .val-tags-table__wrapper 
      h2.val-tags-table__title All Tags
      .val-tags-table__tags-list
        +tagItem('Business')(class='val-tag-item--1')
        +tagItem('Experience')(class='val-tag-item--2')
        +tagItem('Screen')(class='val-tag-item--3')
        +tagItem('Technology')(class='val-tag-item--4')
        +tagItem('Marketing')(class='val-tag-item--5')
        +tagItem('Life')(class='val-tag-item--6')